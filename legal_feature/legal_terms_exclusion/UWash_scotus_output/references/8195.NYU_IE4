<DATE start="74511" citation_local_level_id="203" citation_global_level_id="331823.001" end="74515" id="8195_1488" >1978</DATE>

<COMMENT start="74518" entry_type="comment" line="159" end="74539" id="8195_1479" >opinion of Powell, J.</COMMENT>

