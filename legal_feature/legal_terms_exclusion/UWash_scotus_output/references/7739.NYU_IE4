<CITATION entry_type="case_X_vs_Y" id1="Dummy_File_327427" start="102768" alternative_keys="['u.s._426_26']" line="125" party2_short="Organization" end="102816" citation_local_level_id="251" party1="Simon" party2="Eastern Ky. Welfare Rights Organization" id="7739_1899" citation_global_level_id="327426.004" party1_short="Simon" >Simon v. Eastern Ky. Welfare Rights Organization</CITATION>

