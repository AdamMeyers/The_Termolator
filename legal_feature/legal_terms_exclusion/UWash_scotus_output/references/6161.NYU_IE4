<CITATION entry_type="case_X_vs_Y" id1="Dummy_File_263608" start="78238" line="227" party2_short="Pennsylvania" end="78262" citation_local_level_id="202" party1="Blystone" citation_global_level_id="263607.001" id="6161_1474" no_current_file="True" party2="Pennsylvania" party1_short="Blystone" >Blystone v. Pennsylvania</CITATION>

<PERSON start="78238" party1_of="6161_1474" end="78246" id="6161_1477" >Blystone</PERSON>

<GPE start="78250" party2_of="6161_1474" end="78262" id="6161_1478" >Pennsylvania</GPE>

